    <Reference Include="ZepLaoSharp.FFMpeg">
      <HintPath>D:\Working Repositories\ZepLaoSharp\ZepLaoSharp.FFMpeg\bin\Release\net8.0\ZepLaoSharp.FFMpeg.dll</HintPath>
    </Reference>
    <Reference Include="ZepLaoSharp.Commands">
      <HintPath>D:\Working Repositories\ZepLaoSharp\ZepLaoSharp.Commands\bin\Release\net8.0\ZepLaoSharp.Commands.dll</HintPath>
