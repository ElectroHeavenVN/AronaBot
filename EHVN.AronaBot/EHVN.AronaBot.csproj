    </Reference>
    <Reference Include="ZepLaoSharp.FFMpeg">
      <HintPath>D:\Working Repositories\ZepLaoSharp\ZepLaoSharp.FFMpeg\bin\Debug\net8.0\ZepLaoSharp.FFMpeg.dll</HintPath>
    </Reference>
    <Reference Include="ZepLaoSharp.Commands">
      <HintPath>D:\Working Repositories\ZepLaoSharp\ZepLaoSharp.Commands\bin\Debug\net8.0\ZepLaoSharp.Commands.dll</HintPath>
    </Reference>
